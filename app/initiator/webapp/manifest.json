{
    "_version": "1.32.0",
    "sap.app": {
        "id": "initiator",
        "type": "application",
        "i18n":  {
            "bundleUrl": "i18n/i18n.properties",
            "supportedLocales": ["en", "ja"],
            "fallbackLocale": "en"
        },
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "dataSources": {
            "mainService": {
                "uri": "/cpi-api/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "v2": {
                "uri": "/v2/cpi-api/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml",
                    "useBatch": false,
                    "defaultCountMode": "None" 
                }
            },
            "NorthWind":{
                "uri" : "/V3/Northwind/Northwind.svc",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml",
                    "useBatch": false,
                    "defaultCountMode": "None" 
                }
            },
            "SFSF": {
                "uri": "/SFSF/odata/v2/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml",
                    "useBatch": false,
                    "defaultCountMode": "None" 
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "fe-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "trasfer-intiation",
                    "action": "display",
                    "title": "Initiate Transfer Simulation",
                    "subTitle": "Transfer Simulation",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "initiator.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "flexEnabled": true,
            "minUI5Version": "1.96.1",
            "libs": {
                "sap.ui.core": {},
                
                "sap.ui.commons": {},
                "sap.suite.ui.commons": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleUrl": "i18n/i18n.properties",
					"supportedLocales": ["en", "ja"],
					"fallbackLocale": "en"
				}
			},
            "i18n1": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "initiator.i18n.i18n"
                }
            },
            "oData": {
                "dataSource": "v2",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true,
                    "useBatch" : false
                }
            },
            "SFSF": {
                "dataSource": "SFSF",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "initiator.view",
                "controlAggregation": "content",
                "controlId": "page",    
                "clearControlAggregation": true
            },
            "routes": [
                {
                    "pattern": ":layout:",
                    "name": "EmployeeList",
                    "target": [
                        "EmployeeList"
                    ]
                },
                {
                    "pattern": "Employee/{ID}/:layout:",
                    "name": "OpenPositions",
                    "target": [
                        "OpenPositions"
                    ]
                   
                },
                {
                    "pattern": "EmployeeProfile/{ID}",
                    "name": "EmployeeProfile",
                    "target": [
                        "EmployeeProfile"
                    ]
                   
                },                
                {
                    "name": "RouteApp",
                    "pattern": "RouteApp",
                    "target": [
                        "TargetApp"
                    ]
                }
            ],
            "targets": {
                "EmployeeList": {
                    "controlAggregation": "content",
                    "viewName": "EmployeeList",
                    "viewId": "EmployeeList",
                    "viewLevel": 1,
                    "transition": "slide"
                },
                "OpenPositions": {
                    "controlAggregation": "content",
                    "viewName": "OpenPositions",
                    "viewId": "OpenPositions",
                    "viewLevel": 2,
                    "transition": "slide"
                },
                "EmployeeProfile": {
                    "controlAggregation": "content",
                    "viewName": "EmployeeProfile",
                    "viewId": "EmployeeProfile",
                    "viewLevel": 3,
                    "transition": "slide"
                },                
                "TargetApp": {
                    "viewType": "XML",
                    "transition": "slide",
                  
                    "viewId": "App",
                    "viewName": "App"
                }
            }
        }
    }
}
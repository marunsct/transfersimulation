<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns:f="sap.f" xmlns:fb="sap.ui.comp.filterbar" xmlns:dnd="sap.ui.core.dnd" controllerName="initiator.controller.OpenPositions" id="OpenPositions" displayBlock="false">
    <f:DynamicPage id="dynamicPageId" headerExpanded="{headerExpanded}" showFooter="false">
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>

                <f:heading></f:heading>
                <f:navigationActions>
                    <Button xmlns="sap.m" id="button0_1628578882484" press="_onPageNavButtonPress" icon="sap-icon://nav-back" />
                </f:navigationActions>
                <f:content>
                    <Title text="{i18n>openListTitle}" textAlign="Center" id="titleee" level="H1" />

                </f:content>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="false">
                <f:content>
                    <VBox fitContainer="true">
                        <items>
                            <fb:FilterBar id="filterbar0" search=".onSearch" clear="_onTableItemPress" useToolbar="false" showGoOnFB="true" showClearOnFB="true" showFilterConfiguration="false">
                                <fb:filterGroupItems>
                                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="A" visibleInFilterBar="true" label="{i18n>positionId}">
                                        <fb:control>
                                            <Input validationError="handleValidationError" validationSuccess="handleValidationSuccess" value="{path:'suppInvoiceNo', type: 'sap.ui.model.type.String', constraints: { search: '^[0-9,*,a-z,A-Z]*$' }}" />
                                        </fb:control>
                                    </fb:FilterGroupItem>
                                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="B" label="{i18n>positionTitle}" visibleInFilterBar="true" labelTooltip="{i18n>positionTitle}">
                                        <fb:control>
                                            <Input validationError="handleValidationError" validationSuccess="handleValidationSuccess" value="{path:'suppInvoiceNo', type: 'sap.ui.model.type.String', constraints: { search: '^[0-9,*,a-z,A-Z]*$' }}" />
                                        </fb:control>
                                    </fb:FilterGroupItem>
                                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="C" label="{i18n>positionClass}" visibleInFilterBar="true" labelTooltip="{i18n>positionClass}">
                                        <fb:control>
                                            <Input validationError="handleValidationError" validationSuccess="handleValidationSuccess" value="{path:'suppInvoiceNo', type: 'sap.ui.model.type.String', constraints: { search: '^[0-9,*,a-z,A-Z]*$' }}" />
                                        </fb:control>
                                    </fb:FilterGroupItem>
                                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="D" label="{i18n>positionType}" visibleInFilterBar="true" labelTooltip="{i18n>positionType}">
                                        <fb:control>
                                            <Input validationError="handleValidationError" validationSuccess="handleValidationSuccess" value="{path:'costCenterGrp', type: 'sap.ui.model.type.String'}" />
                                        </fb:control>
                                    </fb:FilterGroupItem>
                                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="E" visibleInFilterBar="true" label="{i18n>positionDepartment}" labelTooltip="{i18n>positionDepartment}">
                                        <fb:control>
                                            <Input validationError="handleValidationError" validationSuccess="handleValidationSuccess" value="{path:'costCenter', type: 'sap.ui.model.type.String'}" />
                                        </fb:control>
                                    </fb:FilterGroupItem>
                                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="F" visibleInFilterBar="true" label="{i18n>positionLocation}" labelTooltip="{i18n>positionLocation}">
                                        <fb:control>
                                            <Input validationError="handleValidationError" validationSuccess="handleValidationSuccess" value="{path:'wbsElement', type: 'sap.ui.model.type.String'}" />
                                        </fb:control>
                                    </fb:FilterGroupItem>
                                </fb:filterGroupItems>
                            </fb:FilterBar>
                        </items>
                    </VBox>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <!-- DynamicPage Content -->
        <f:content>
            <!-- commnt scroll 	
								<ScrollContainer id="sc1" height="100%" width="100%" horizontal="false" vertical="true" focusable="true">
						-->


            <Table sticky="HeaderToolbar,InfoToolbar,ColumnHeaders"  noDataText="{i18n>terror}" id="table0" items="{OP>/OpenPositions/result}" fixedLayout="true" includeItemInSelection="false" growingScrollToLoad="true" growing="true" growingThreshold="25" updateFinished="onUpdateFinished" updateStarted="onUpdateStarted" growingTriggerText="..." itemPress="_onTableItemPress"  mode="SingleSelectMaster">
            <!--
                <dragDropConfig>
                    <dnd:DropInfo groupName="available2selected" targetAggregation="items" dropPosition="Between" drop="onDropSelectedEmployee" />
                    <dnd:DragDropInfo sourceAggregation="items" targetAggregation="items" dropPosition="Between" drop="onDropSelectedEmployee" />
                </dragDropConfig>
                -->
                <items>
                    <ColumnListItem type="Active" id="item0">
                        <cells>
                            <ObjectIdentifier title="{OP>Name1}" text="{OP>ID1}" titleActive="false" visible="true" />
                            <!--ObjectIdentifier title="{ (= ${OP>positionTitle}!== null || OP>positionTitle}!=='' ) ? ${OP>positionTitle} : ${externalName_defaultValue} }" text="{OP>code}" titleActive="false" visible="true" / -->
                            <ObjectIdentifier title="{OP>externalName_defaultValue}" text="{OP>code}" titleActive="false" visible="true" />
                            <ObjectIdentifier title="Employment Class" text="{OP>employeeClass}" titleActive="false" visible="true" />
                            <ObjectIdentifier title="Employment Type" text="{OP>cust_employmentType}" titleActive="false" visible="true" />
                            <ObjectIdentifier title="Department Name" text="{OP>department}" titleActive="false" visible="true" />
                            <ObjectIdentifier title="Location" text="{OP>location}" titleActive="false" visible="true" />
                            <!--
                            <Text text="{OP>employeeClass}" id="text3" wrapping="true" />
                            <Text text="{OP>cust_employmentType}" id="text4" wrapping="true" />
                            <Text text="{OP>department}" id="text5" wrapping="true" />
                            <Text text="{OP>location}" id="text6" wrapping="true" />
                            -->
                            <Text text="{OP>standardHours}" id="text7" wrapping="true" />
                        </cells>
                    </ColumnListItem>
                </items>
                <columns>
                    <Column id="column0" minScreenWidth="Phone" popinDisplay="Inline">
                        <header>
                            <Label id="label0" text="{i18n>employee}" wrapping="true"/>
                        </header>
                    </Column>
                    <Column id="column1" minScreenWidth="Phone">
                        <header>
                            <Label text="{i18n>position}" id="label1" wrapping="true"/>
                        </header>
                    </Column>
                    <Column id="column2" minScreenWidth="Phone" >
                        <header>
                            <Label text="{i18n>positionDepartment}" id="label2" wrapping="true"/>
                        </header>
                    </Column>
                    <Column id="column3" minScreenWidth="Tablet" popinDisplay="Inline">
                        <header>
                            <Label text="{i18n>positionClass}" id="label3" wrapping="true"/>
                        </header>
                    </Column>
                    <Column id="column4" minScreenWidth="Tablet">
                        <header>
                            <Label text="{i18n>positionType}" id="label4" wrapping="true"/>
                        </header>
                    </Column>
                    <Column id="column5" minScreenWidth="Tablet" popinDisplay="Inline" >
                        <header>
                            <Label text="{i18n>positionLocation}" id="label5" wrapping="true"/>
                        </header>
                    </Column>
                    <Column id="column6" minScreenWidth="Tablet">
                        <header>
                            <Label text="{i18n>standardHours}" id="label6" wrapping="true"/>
                        </header>
                    </Column>
                </columns>
                <headerToolbar>
                    <OverflowToolbar width="100%" id="toolbar2" class="sapUiLargeMarginEnd">
                        <content>
                            <Text text="{i18n>posiitoTH}" id="text06" />
                            <ToolbarSpacer id="spacer2" />
                            <!-- <Button id="button0_1579056670196" press="onNavtoDetail" text="{i18n>tbnew}"/> -->
                            <Button id="button0" icon="sap-icon://sort" press="handleSortButtonPressed" />
                            <Button id="button2" press="onPersoButtonPressed" icon="sap-icon://action-settings" />
                            <Button id="button4" icon="sap-icon://excel-attachment" press="onExport" enabled="false" class="sapUiLargeMarginEnd" />
                        </content>
                    </OverflowToolbar>
                </headerToolbar>
            </Table>

            <!-- commnt scroll
				</ScrollContainer>
							<content/> <Text text="{i18n>theader} {/reply/hNumbers}" id="text06"/>
						
						-->
        </f:content>
        <!-- DynamicPage Footer -->
        <f:footer />
    </f:DynamicPage>
</mvc:View>
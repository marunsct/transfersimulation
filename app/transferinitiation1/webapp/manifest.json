{
    "_version": "1.32.0",
    "sap.app": {
        "id": "transferinitiation",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "dataSources": {
            "mainService": {
                "uri": "/v2/cpi-api/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "fe-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "trasfer-intiation",
                    "action": "display",
                    "title": "Initiate Transfer Simulation",
                    "subTitle": "Transfer Simulation",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "transferinitiation.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "transferinitiation.i18n.i18n"
                }
            },
            "oData": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "transferinitiation.view",
                "controlAggregation": "beginColumnPages",
                "controlId": "idAppControl",    
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "pattern": ":layout:",
                    "name": "EmployeeList",
                    "target": [
                        "EmployeeList"
                    ],
                    "layout": "OneColumn"
                },
                {
                    "pattern": "Employee/{ID}/:layout:",
                    "name": "OpenPositions",
                    "target": [
                        "EmployeeList",
                        "OpenPositions"
                    ],
                    "layout": "TwoColumnsMidExpanded"
                },                
                {
                    "name": "RouteApp",
                    "pattern": "RouteApp",
                    "target": [
                        "TargetApp"
                    ]
                }
            ],
            "targets": {
                "EmployeeList": {
                    "controlAggregation": "beginColumnPages",
                    "viewName": "EmployeeList",
                    "viewId": "EmployeeList",
                    "viewLevel": 3,
                    "transition": "slide"
                },
                "OpenPositions": {
                    "controlAggregation": "midColumnPages",
                    "viewName": "OpenPositions",
                    "viewId": "OpenPositions",
                    "viewLevel": 4,
                    "transition": "slide"
                },                
                "TargetApp": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "App",
                    "viewName": "App"
                }
            }
        }
    }
}
<mvc:View
    controllerName="transferinitiation.controller.App"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="false"
    xmlns="sap.m"
    xmlns:f="sap.f" 
    busy="{appView>/busy}" 
    busyIndicatorDelay="{appView>/delay}" 
    height="100%">
	<Shell id="shell" appWidthLimited="false">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					 <f:FlexibleColumnLayout id="idAppControl" stateChange="onStateChanged" layout="{FclRouter>/uiState/layout}"/>
				</Page>
			</pages>        
		</App>
	</Shell>
</mvc:View>

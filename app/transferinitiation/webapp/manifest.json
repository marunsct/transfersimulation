{
    "_version": "1.32.0",
    "sap.app": {
        "id": "transferinitiation",
        "type": "application",
        "i18n": {
            "bundleUrl": "i18n/i18n.properties",
            "supportedLocales": [
                "en",
                "ja"
            ],
            "fallbackLocale": "en"
        },
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "dataSources": {
            "mainService": {
                "uri": "/SFSF/odata/v2/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "fe-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "trasfer-intiation",
                    "action": "display",
                    "title": "Initiate Transfer Simulation",
                    "subTitle": "Transfer Simulation",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "transferinitiation.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "oData": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "transferinitiation.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "transferinitiation.view",
                "controlAggregation": "beginColumnPages",
                "controlId": "idAppControl",   
                "clearTarget": false, 
                "bypassed": {
					"target": [
						"EmployeeList"
					]
				}
            },
            "routes": [
                {
                    "pattern": "EmployeeList/:layout:",
                    "name": "EmployeeList",
                    "target": [
                        "EmployeeList"
                    ],
                    "layout": "OneColumn"
                },
                {
                    "pattern": "Employee/{ID}/:layout:",
                    "name": "OpenPositions",
                    "target": [
                        "EmployeeList",
                        "OpenPositions"
                    ],
                    "layout": "TwoColumnsMidExpanded"
                }, 
                {
                    "pattern": "EmployeeProfile/{ID}",
                    "name": "EmployeeProfile",
                    "target": [
                        "EmployeeProfile"
                    ],
                    "layout": "OneColumn"
                },
				{
					"pattern": ""  ,
					"name": "default",
					"target": [
						"EmployeeList"
					],
					"layout": "OneColumn"
				}
            ],
            "targets": {
                "EmployeeList": {
                    "controlAggregation": "beginColumnPages",
                    "viewName": "EmployeeList",
                    "viewId": "EmployeeList",
                    "viewLevel": 3  ,
                    "transition": "slide"
                },
                "EmployeeProfile": {
                    "controlAggregation": "beginColumnPages",
                    "viewName": "EmployeeProfile",
                    "viewId": "EmployeeProfile",
                    "viewLevel": 1,
                    "transition": "slide"
                },
                "OpenPositions": {
                    "controlAggregation": "midColumnPages",
                    "viewName": "OpenPositions",
                    "viewId": "OpenPositions",
                    "viewLevel": 4,
                    "transition": "slide"
                },                
                "TargetApp": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "App",
                    "viewName": "App"
                }
            }
        }
    }
}
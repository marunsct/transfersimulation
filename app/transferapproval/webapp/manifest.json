{
    "_version": "1.32.0",
    "sap.app": {
        "id": "transferapproval",
        "type": "application",
        "i18n": {
            "bundleUrl": "i18n/i18n.properties",
            "supportedLocales": [
                "en",
                "ja"
            ],
            "fallbackLocale": "en"
        },
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "dataSources": {
            "mainService": {
                "uri": "/cpi-api/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "v2": {
                "uri": "/v2/cpi-api/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml",
                    "useBatch": false,
                    "defaultCountMode": "None"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "transferapproval.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "dependencies": {
            "minUI5Version": "1.96.1",
            "libs": {
                "sap.ui.core": {},
                "sap.ui.commons": {},
                "sap.suite.ui.commons": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "transferapproval.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true
            },
            "i18n1": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "initiator.i18n.i18n"
                }
            },
            "oData": {
                "dataSource": "v2",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true,
                    "useBatch": false
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "transferapproval.view",
                "controlAggregation": "content",
                "controlId": "page",
                "clearControlAggregation": true
            },
            "routes": [
                {
                    "name": "RouteView1",
                    "pattern": "RouteView1",
                    "target": [
                        "TargetView1"
                    ]
                },
                {
                    "pattern": "",
                    "name": "TransferList",
                    "target": [
                        "TransferList"
                    ]
                },
                {
                    "pattern": "TransferDetail/{ID}",
                    "name": "TransferDetail",
                    "target": [
                        "TransferDetail"
                    ]
                },
                {
                    "pattern": "OpenPositions/{ID}",
                    "name": "OpenPositions",
                    "target": [
                        "OpenPositions"
                    ]
                },
                {
                    "pattern": "EmployeeProfile/{ID}",
                    "name": "EmployeeProfile",
                    "target": [
                        "EmployeeProfile"
                    ]
                }
                
            ],
            "targets": {
                "TargetView1": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "App",
                    "viewName": "App"
                },
                "TransferList": {
                    "controlAggregation": "content",
                    "viewName": "TransferList",
                    "viewId": "TransferList",
                    "viewLevel": 1,
                    "transition": "slide"
                },
                "TransferDetail": {
                    "controlAggregation": "content",
                    "viewName": "TransferDetail",
                    "viewId": "TransferDetail",
                    "viewLevel": 2,
                    "transition": "slide"
                },
                "OpenPositions": {
                    "controlAggregation": "content",
                    "viewName": "OpenPositions",
                    "viewId": "OpenPositions",
                    "viewLevel": 3,
                    "transition": "slide"
                },
                "EmployeeProfile": {
                    "controlAggregation": "content",
                    "viewName": "EmployeeProfile",
                    "viewId": "EmployeeProfile",
                    "viewLevel": 3,
                    "transition": "slide"
                }
                
            }
        }
    }
}


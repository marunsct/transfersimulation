{
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/http/(.*)$",
            "target": "/http/$1",
            "destination": "CPI_DEV",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },
        {
            "source": "^/upsert",
            "target": "/odata/v2/upsert",
            "destination": "SFSF",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },
        {
            "source": "^/SFSF/odata/(.*)$",
            "target": "/odata/$1",
            "destination": "SFSF",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },

        {
            "source": "/v2/(.*)",
            "target": "/v2/$1",
            "destination": "srv-api",
            "csrfProtection": false,
            "authenticationType": "xsuaa"
        },
        {
            "source": "/cpi-api-v4/(.*)$",
            "target": "/cpi-api-v4/$1",
            "destination": "srv-api",
            "csrfProtection": false,
            "authenticationType": "xsuaa"
        },
        {
            "source": "/cpi-api/(.*)$",
            "target": "/cpi-api/$1",
            "destination": "srv-api",
            "csrfProtection": false,
            "authenticationType": "xsuaa"
        },
        {
            "source": "^/user-api(.*)",
            "target": "$1",
            "service": "sap-approuter-userapi"
        },
        {
            "source": "^/loggedout.html$",
            "localDir": ".",
            "authenticationType": "none"
          },
        {
            "source": "/(.*)",
            "localDir": ".",
            "authenticationType": "xsuaa",
            "cacheControl": "no-cache, no-store, must-revalidate"
        }
    ],
    "logout": {
        "logoutEndpoint": "/logout",
        "logoutPage": "/loggedout.html"
     },
    "welcomeFile": "/transferinitiation/index.html"
}
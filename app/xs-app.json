 {
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/http/(.*)$",
            "target": "/http/$1",
            "destination": "CPI_DEV",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },
        {
            "source": "^/upsert",
            "target": "/odata/v2/upsert",
            "destination": "SFSF",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },
        {
            "source": "^/SFSF/odata/(.*)$",
            "target": "/odata/$1",
            "destination": "SFSF",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },
        {
            "source": "^/user-api(.*)",
            "target": "$1",
            "service": "sap-approuter-userapi"
        },
        {
            "source": "^/loggedout.html$",
            "localDir": ".",
            "authenticationType": "none"
          },
        {
            "source": "/(.*)",
            "localDir": ".",
            "authenticationType": "xsuaa",
            "scope":{
                "GET":["$XSAPPNAME.user"],
                "PATCH":["$XSAPPNAME.user"],
                "POST":["$XSAPPNAME.user"],
                "PUT":["$XSAPPNAME.user"],
                "DELETE":["$XSAPPNAME.user"],
                "default":["$XSAPPNAME.user"]
              },
            "cacheControl": "no-cache, no-store, must-revalidate"
        }
    ],
    "logout": {
        "logoutEndpoint": "/logout",
        "logoutPage": "/loggedout.html"
     },
    "welcomeFile": "/transferinitiation/index.html"
}